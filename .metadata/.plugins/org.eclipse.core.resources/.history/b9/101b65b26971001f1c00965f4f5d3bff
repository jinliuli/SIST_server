	public static void main(String[] args) {
		//
		
		try {
			
			//https
			
			String path = ".\\src\\main\\webapp\\time.html";
			
			BufferedWriter writer = new BufferedWriter(new FileWriter(path)); 
			
			writer.write("<html>);
			writer.write("<head>);
			writer.write("<>);
			writer.write("<>);
			writer.write("<>);
					
					
		} catch (Exception e) {
			System.out.println("Ex01.main");
			e.printStackTrace();
		}
	}
}
